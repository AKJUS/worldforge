<atlas>
    <map>
        <map name="attributes">
            <map name="bbox">
                <list name="default">
                    <float>-0.4</float>
                    <float>0</float>
                    <float>-0.4</float>
                    <float>0.4</float>
                    <float>1.8</float>
                    <float>0.4</float>
                </list>
            </map>
            <map name="geometry">
                <map name="default">
                    <string name="path">dural/creatures/biped/male/model/male.mesh</string>
                    <string name="type">capsule-y</string>
                </map>
            </map>
            <map name="present">
                <string name="default">dural/creatures/biped/human.entitymap</string>
            </map>
            <map name="attachments">
                <map name="default">
                    <string name="hand_primary">contains_recursive(actor.contains, child = tool)</string>
                    <string name="hand_secondary">contains_recursive(actor.contains, child = tool)</string>
                    <string name="hand_torso">actor contains tool &amp;&amp; tool.props.worn = 'torso'</string>
                    <string name="hand_legs">actor contains tool &amp;&amp; tool.props.worn = 'legs'</string>
                    <string name="hand_feet">actor contains tool &amp;&amp; tool.props.worn = 'feet'</string>
                    <string name="hand_head">actor contains tool &amp;&amp; tool.props.worn = 'head'</string>
                </map>
            </map>
            <map name="decays">
                <string name="default">dead_human_skull</string>
            </map>
            <map name="guise">
                <map name="default">
                    <map name="material">
                        <string name="head">simon</string>
                    </map>
                </map>
            </map>
            <map name="sex">
                <string name="default">male</string>
            </map>
            <map name="statistics">
                <map name="default">
                    <float name="strength">23</float>
                </map>
            </map>
            <!-- Don't move too slow or it makes traversal boring -->
            <map name="speed_ground">
                <float name="default">8</float>
            </map>
        </map>
        <string name="id">human</string>
        <string name="objtype">class</string>
        <string name="parent">mobile</string>
    </map>
</atlas>
