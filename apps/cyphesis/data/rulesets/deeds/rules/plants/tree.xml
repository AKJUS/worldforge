                    </map>
                    <map>
                        <string name="language">python</string>
                        <string name="name">world.traits.Loggable.Loggable</string>
                    </map>
                    <map>
                        <string name="language">python</string>
                        <string name="name">world.traits.StatusDeletable.StatusDeletable</string>
                    </map>
                </list>
            </map>
        </map>
        <string name="id">tree</string>
        <string name="objtype">class</string>
        <string name="parent">plant</string>
    </map>
